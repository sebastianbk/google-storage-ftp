<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>netcoreapp2.2</TargetFramework>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="FubarDev.FtpServer" Version="3.0.0-beta.5" />
		<PackageReference Include="FubarDev.FtpServer.Abstractions" Version="3.0.0-beta.5" />
		<PackageReference Include="Google.Cloud.Logging.NLog" Version="2.0.0" />
		<PackageReference Include="Google.Cloud.Storage.V1" Version="2.3.0" />
		<PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="2.2.0" />
		<PackageReference Include="NLog" Version="4.6.0" />
		<PackageReference Include="NLog.Extensions.Logging" Version="1.4.0" />
	</ItemGroup>

	<ItemGroup>
		<Content Include="NLog.config">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Content>
	</ItemGroup>

</Project>
